{
  "name": "electron-remote-actions",
  "version": "1.0.3",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start": "yarn self-link && yarn tsc -w",
    "watch": "jest --watch",
    "test": "yarn self-link && yarn build && jest",
    "release":
      "yarn build && git commit -am 'build'; yarn version && git push --tags && git push origin master",
    "build": "yarn tsc",
    "self-link": "yarn link && yarn link electron-remote-actions"
  },
  "peerDependencies": {
    "electron": "^2.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^5.0.0",
    "@types/inquirer": "^0.0.36",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.102",
    "@types/node": "^9.4.4",
    "@types/ora": "^1.3.1",
    "electron": "^2.0.0",
    "jest": "^22.4.3",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.2",
    "spectron": "^3.8.0",
    "ts-jest": "^22.4.4",
    "tslint-config-prettier": "^1.7.0",
    "typescript": "^2.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "src/__tests__/.*\\.(test|spec)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  }
}
